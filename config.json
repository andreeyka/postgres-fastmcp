{
    "name": "postgres-fastmcp",
    "mask_error_details": true,
    "endpoint": "mcp",
    "transport": "http",
    "databases": {
        "admin_rw_db": {
            "database_uri": "postgresql://postgres:postgres@postgres:5432/admin_rw_db",
            "streamable": false,
            "access_mode": "admin_rw"
        },
        "user_db": {
            "database_uri": "postgresql://postgres:postgres@postgres:5432/user_db",
            "streamable": false,
            "access_mode": "user_ro",
            "table_prefix": "app_"
        }
    },
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 1
}